<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <AssemblyName>Aiursoft.BaGet.Database.Sqlite</AssemblyName>
    <RootNamespace>Aiursoft.BaGet.Database.Sqlite</RootNamespace>
    <IsTestProject>false</IsTestProject>
    <IsPackable>false</IsPackable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Description>The libraries to host BaGet on SQLite.</Description>
    <PackageTags>NuGet</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="8.0.6" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\BaGet.Core\Aiursoft.BaGet.Core.csproj" />
  </ItemGroup>
</Project>