{
  "ApiKey": "default-key",
  "PackageDeletionBehavior": "Unlist",
  "AllowPackageOverwrites": false,

  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db;Cache=Shared"
  },
  "Storage": {
    "Type": "FileSystem",
    "Path": "/data"
  },
  "Search": {
    "Type": "Database"
  },
  "Mirror": {
    "Enabled": true,
    "Legacy": false,
    "PackageSource": "https://api.nuget.org/v3/index.json"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
